{
  "manifest_version": 2,
  "name": "Instapaper Reloaded",
  "version": "1.0.0",
  "description": "Some tweaks to instapaper.com",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["http://*.instapaper.com/*"],
      "css": ["assets/all.css"],
      "js": ["assets/all.js"]
    },

    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["assets/opener.js"]
    }
  ]
}


